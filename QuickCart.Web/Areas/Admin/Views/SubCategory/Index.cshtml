

@{
    ViewBag.PageTitle = "Sub Category Mangment";
    ViewBag.CardTitle = "Sub Categories ";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}


<div class="mb-3">
    <a asp-action="Create" asp-controller="SubCategory" class="btn btn-primary">New Sub Category</a>
</div>

<table id="subCategoryDataTable" class="display" style="width:100%">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Description</th>
            <th>CreatedAt</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
    </tbody>
</table>

@section Scripts {
    <script src="~/scripts/area/admin/subcategory/subcategorylist.js"></script>

}